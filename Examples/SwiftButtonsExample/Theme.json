{
    "$RedColor": "#f93d38",
    "$BlueColor": "#50b5ed",
    "$WhiteColor": "#f1efeb",
    "$RegularFontName": "AvenirNext-Regular",
    "$H5FontSize": 16,
    ".ContentBackground": {
        "backgroundColor": "$WhiteColor"
    },
    ".Button": {
        "fontName": "$RegularFontName",
        "fontSize": "$H5FontSize",
        "contentEdgeInsets": "{10.0, 20.0, 10.0, 20.0}",
        "borderWidth": 1.0,
        "cornerRadius": 5.0
    },
    ".DestructiveButton": {
        "_superclass": ".Button",
        "tintColor": "$RedColor",
        "borderColor": "$RedColor"
    },
    ".PrimaryButton": {
        "_superclass": ".Button",
        "tintColor": "$BlueColor",
        "borderColor": "$BlueColor"
    },
    ".ButtonsView": {
        "_superclass": ".ContentBackground",
        "saveButton": ".PrimaryButton",
        "deleteButton": ".DestructiveButton"
    }
}
